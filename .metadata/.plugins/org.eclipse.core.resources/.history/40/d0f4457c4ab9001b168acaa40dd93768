	pageEncoding="UTF-8"%>
<jsp:useBean id="student" class="ex01.Student" scope="request"></jsp:useBean>
<%-- 
	위의 useBean 코드는 아래 코드와 똑같음.
    Student student = new Student();
    request.setAttribute("student",student);
    id는 키값과 같음.
     --%>
